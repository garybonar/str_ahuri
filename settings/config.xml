        <input type="radio" required="true">Under 25;;;26-35;;;36-45;;;46-55;;;56-65;;;66+</input>
        <label>11. What is your age?</label>
        <note></note>
   </item>

   <item id="survey">
        <input type="text" required="true" limited="false" limitLength="99999" restricted="false"></input>
        <label>12. What is your gender? (type in your response)</label>
        <note></note>
        <placeholder></placeholder>
   </item>

   <item id="survey">
        <input type="radio" required="true">Under $75,000;;;$75,001 - $100,000;;;$100,001 - $150,000;;;$150,001 - $200,000;;;$200,001 - $250,000;;;$250,001- $300,000;;;$300,000+;;;Prefer not to disclose</input>
